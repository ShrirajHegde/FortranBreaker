{
	"name": "fortranbreaker",
	"displayName": "Fortran Breakpoint Support",
	"description": "Add breakpoint support for fortran",
	"version": "0.0.4",
	"publisher": "ekibun",
	"icon": "FortranBreaker_128x.png",
	"readme": "README.md",
	"engines": {
		"vscode": "^1.21.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ekibun/FortranBreaker.git"
	},
	"homepage": "https://github.com/ekibun/FortranBreaker",
	"qna": "https://github.com/ekibun/FortranBreaker/issues",
	"categories": [
		"Debuggers"
	],
	"extensionDependencies": [
		"ms-vscode.cpptools"
	],
	"contributes": {
		"debuggers": [
			{
				"type": "cppdbg",
				"label": "Fortran (GDB)",
				"enableBreakpointsFor": {
					"languageIds": [
						"fortran", "fortran-modern", "FortranFreeForm", "fortran_fixed-form"
					]
				}
			},
			{
				"type": "cppvsdbg",
				"label": "Fortran (MSVC)",
				"enableBreakpointsFor": {
					"languageIds": [
						"fortran", "fortran-modern", "FortranFreeForm", "fortran_fixed-form"
					]
				}
			}
		]
	}
}
